Cell[BoxData[{
    \(\(h0 = IntegerPart[hcon];\)\), "\[IndentingNewLine]", 
    \(\(x0 = \((p\[Alpha]recL[h0] - p\[Alpha]reccentsom[h0])\)*Degree*
          Cos[pdecL[h0] Degree]\ ;\)\), "\[IndentingNewLine]", 
    \(\(y0 = \((pdecL[h0] - pdeccentsom[h0])\)*
          Degree;\)\), "\[IndentingNewLine]", 
    \(\(xc = \((p\[Alpha]recL[hcon] - p\[Alpha]reccentsom[hcon])\)*Degree*
          Cos[pdecL[hcon] Degree]\ ;\)\), "\[IndentingNewLine]", 
    \(\(yc = \((pdecL[hcon] - pdeccentsom[hcon])\)*
          Degree;\)\), "\[IndentingNewLine]", 
    \(\(xp = \(xc - x0\)\/\(hcon - h0\);\)\), "\[IndentingNewLine]", 
    \(\(yp = \(yc - y0\)\/\(hcon - h0\);\)\), "\[IndentingNewLine]", 
    \(\(mecl = \@\(x0\^2 + y0\^2\);\)\), "\[IndentingNewLine]", 
    \(\(necl = \@\(xp\^2 + yp\^2\);\)\), "\[IndentingNewLine]", 
    \(\(Mecl = ArcTan[y0, x0];\)\), "\[IndentingNewLine]", 
    \(\(Necl = ArcTan[yp, xp];\)\)}], "Input"]
