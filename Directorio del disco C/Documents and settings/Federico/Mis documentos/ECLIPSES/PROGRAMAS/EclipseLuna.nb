Cell[BoxData[
    \(deltamenosLt[{lon_, lat_, t_}] := 
      Map[deltamenosL[{lon, lat, #}] &, listat[t]]\)], "Input"],

Cell[BoxData[
    RowBox[{\(General::"spell1"\), \(\(:\)\(\ \)\), "\<\"Possible spelling \
