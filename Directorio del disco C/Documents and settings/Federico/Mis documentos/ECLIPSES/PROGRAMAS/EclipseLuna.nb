Cell[BoxData[
    \(radeliphor[d_] := 
      Module[{a, f, \[Phi], u, x, z, r}, a = 1; 
        f = 1\/298.257; \[Phi] = \((90 - d)\) Degree; 
        u = ArcTan[\((1 - f)\) Cos[\[Phi]], Sin[\[Phi]]]; x = a*Cos[u]; 
        z = a*\((1 - f)\)*Sin[u]; r = \@\(x\^2 + z\^2\)]\)], "Input"],

Cell[BoxData[
    \(disEjeElip[{x_, y_, b_, u_}] := 
      Sign[Abs[y] - Abs[b\ Sin[u]]] 
        Sqrt[\((y - b\ Sin[u])\)^2 + \((x - Cos[u])\)^2]\)], "Input"]
}, Closed]],

