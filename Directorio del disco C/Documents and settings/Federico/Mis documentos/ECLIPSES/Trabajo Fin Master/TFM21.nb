Cell["\<\
Clear[ejes]
ejes[{l_, f_}] := ejes[{l, f}] = 
             Module[{ld = N[l Degree], fd = N[f Degree]},
               {{Cos[ld] Sin[fd], -Sin[ld], Cos[ld] Cos[fd]}, 
                {Sin[ld] Sin[fd],  Cos[ld], Sin[ld] Cos[fd]}, 
                {       -Cos[fd],       0,          Sin[fd]}}]                \
\>", "Input",
 InitializationCell->True,
 CellChangeTimes->{{3.480656938234375*^9, 3.48065695321875*^9}},
 ImageRegion->{{0, 1}, {0, 1}}],

Cell[BoxData[
 RowBox[{"(*", 
  RowBox[{"Orienta", " ", "los", " ", "ejes", " ", "del", " ", "dibujo"}], 
  "*)"}]], "Input",
 CellChangeTimes->{{3.480656971671875*^9, 3.48065698228125*^9}}],

Cell["\<\
Clear[coor];
coor[p_List][{l_, f_}] :=
  Module[
    {ld = N[l Degree], fd = N[f Degree], x, y, z, r},
    x = Cos[fd] Cos[ld]; 
    y = Cos[fd] Sin[ld]; 
    z = Sin[fd];
    {x, y, z} = {x, y, z} . ejes[p];
    {ld, fd} = {ArcTan[x, y], ArcSin[z]};
    r = N[2 Tan[1/2 (Pi/2 - fd)]];
    r {Sin[ld], -Cos[ld]}]    \
\>", "Input",
 InitializationCell->True,
 CellChangeTimes->{{3.4806569575*^9, 3.48065696665625*^9}},
 ImageRegion->{{0, 1}, {0, 1}}],

